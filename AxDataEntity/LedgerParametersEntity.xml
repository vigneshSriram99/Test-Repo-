		<Methods>
			<Method>
				<Name>postLoad</Name>
				<Source><![CDATA[
    public void postLoad()
    {
        super();

        // If the EnableLedgerSettlement parameter has a value of "Unknown", this is equivalent to "Yes"
        // as this represents that the Ledger settlements feature is enabled by default (this parameter was added
        // to opt out of an existing feature)
        if (this.EnableLedgerSettlement == UnknownNoYes::Unknown)
        {
            this.EnableLedgerSettlement = UnknownNoYes::Yes;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initializeEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Map the OData excel fields to the corresponding entity fields
    /// </summary>
    /// <param name = "_entityCtx">Entity context</param>
    /// <param name = "_dataSourceCtx">Entity DataSource context</param>
    public void initializeEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        super(_entityCtx, _dataSourceCtx);

        if (this.UsePostingDefinitions == NoYes::No)
        {
            this.UseBudgetAppropriation = NoYes::No;
            this.UseEncumbranceProcess = NoYes::No;
            this.UsePreencumbranceProcess = NoYes::No;
            this.UseBudgetReservationProcess = NoYes::No;
        }

    }

]]></Source>
			</Method>
		</Methods>
