				<Source><![CDATA[
    /// <summary>
    /// This method ensures that the LogisticsPostalAddress RecId is set correctly on the AssetLocation table
    /// when a AssetLocationEntity record is inserted.
    /// </summary>
    /// <param name = "_entityCtx">The <c>DataEntityRuntimeContext</c> instance.</param>
    /// <param name = "_dataSourceCtx">The <c>DataEntityDataSourceRuntimeContext</c> instance.</param>
    /// <returns>True if the record was inserted successfully, false if it was not.</returns>
    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret;

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(AssetLocationEntity, AssetLocation))
        {
            LogisticsLocationRecId locationRecId = LogisticsPostalAddressBaseEntity::saveAddress(_entityCtx,
                dataEntityDataSourceStr(AssetLocationEntity, LogisticsPostalAddressBaseEntity));

            AssetLocation assetLocation = _dataSourceCtx.getBuffer();
            assetLocation.LogisticsLocation = locationRecId;
        }

        ret = super(_entityCtx, _dataSourceCtx);

        return ret;
    }

]]></Source>
