			<Method>
				<Name>jumpRefMethod</Name>
				<Source><![CDATA[
    public static void jumpRefMethod(str _menuItemName, Common _stagingBuffer, Object _callerObj, DataAreaId _dataAreaId)
    {
        changeCompany(_dataAreaId)
        {
            ReturnOrderLineStaging stagingRecord = _stagingBuffer;

            SalesTable returnOrder = SalesTable::find(stagingRecord.ReturnOrderNumber);

            Args args = new Args();
            args.record(returnOrder);
            args.lookupRecord(returnOrder);

            MenuFunction::runCalled(_menuItemName, MenuItemType::Display, false, args);
        }
    }

]]></Source>
