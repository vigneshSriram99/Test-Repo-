			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean isValid = this.isJournalValid();

        if (isValid)
        {
			// skip setting default dimension display value if it is blank otherwise isFieldSet will set to true and further we can't set default dimension as it remains blank
			if (this.DefaultDimensionDisplayValue != '')
			{
            	this.DefaultDimensionDisplayValue = LedgerJournalLineEntity::validateDefaultDimensionDisplayValueIsNonLedgerType(this.AccountType, this.DefaultDimensionDisplayValue);
			}

			if (this.OffsetDefaultDimensionDisplayValue != '')
			{
            	this.OffsetDefaultDimensionDisplayValue = LedgerJournalLineEntity::validateDefaultDimensionDisplayValueIsNonLedgerType(this.OffsetAccountType, this.OffsetDefaultDimensionDisplayValue, true);
			}

            isValid = super();
        }

        return isValid;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateDefaultDimensionDisplayValueIsNonLedgerType</Name>
				<Source><![CDATA[
    /// <summar
    /// Warns if the default dimension is set when the account type is ledger. 
    /// </summary>
    /// <param name = "_accountType">The account type.</param>
    /// <param name = "_displayValue">The default dimension display value.</param>
    /// <param name = "_isOffset">true if checking the offset field, otherwise, false.</param>
    /// <returns>The resulting default dimension display value.</returns>
    internal static DimensionDisplayValue validateDefaultDimensionDisplayValueIsNonLedgerType(DimensionAccountType _accountType, DimensionDisplayValue _displayValue, boolean _isOffset = false)
    {
        DimensionDisplayValue result = _displayValue;

        if (_accountType == LedgerJournalACType::Ledger && _displayValue != '')
        {
            FieldName displayValueFieldName = _isOffset ? fieldPName(LedgerJournalLineEntity, OffsetDefaultDimensionDisplayValue) : fieldPName(LedgerJournalLineEntity, DefaultDimensionDisplayValue); 
            FieldName accountTypeFieldName = _isOffset ? fieldPName(LedgerJournalLineEntity, OffsetAccountType) : fieldPName(LedgerJournalLineEntity, AccountType);
            
            // Don't fail, only throw a warning.
            warning(strFmt("@GeneralLedger:JournalEntityDimensionsOnLedgerAccount",
                displayValueFieldName,
                accountTypeFieldName,
                enum2Str(LedgerJournalACType::Ledger)));

            result = '';
        }

        return result;
    }

]]></Source>
